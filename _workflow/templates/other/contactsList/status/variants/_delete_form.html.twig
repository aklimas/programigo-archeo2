<form method="post" action="{{ path('contactsListStatusVariants_delete', {'status': contactsList_status.id, 'id': contactsList_status_variants.id}) }}" onsubmit="return confirm('Czy jesteś pewny, że chcesz usunąć wpis?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contactsList_status_variants.id) }}">
    <button class="btn btn-sm btn-danger">{% trans %}Usuń{% endtrans %}</button>
</form>
